# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def isPalindrome(self, head: Optional[ListNode]) -> bool:
        l = list()
        temp = head
        while temp!=None:
            l.append(temp.val)
            temp = temp.next
        i = 0 
        j = len(l)-1
        while i<=j:
            if l[i]!=l[j]:
                return False
            i = i+1
            j = j-1
        return True
