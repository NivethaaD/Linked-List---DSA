# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def removeNthFromEnd(self, head: Optional[ListNode], n: int) -> Optional[ListNode]:
        temp = head
        l = list()
        while temp != None:
            l.append(temp)
            temp = temp.next
        llen = len(l)
        if llen == 1 and n==1:
            return None
        for i in range(0,llen):
            if llen - i == n:
                dummy = l[i].next
                if i-1 >= 0:
                    l[i-1].next = dummy
                else:
                    head = dummy
                break
        return head


