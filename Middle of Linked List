# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def middleNode(self, head: Optional[ListNode]) -> Optional[ListNode]:
        l = list()
        temp = head
        while temp!= None:
            l.append(temp)
            temp = temp.next
        n = len(l)
        m = n//2
        return l[m]
