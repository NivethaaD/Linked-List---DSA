'''
class Node:
	def __init__(self, data):
		self.data = data
		self.next = None
		self.prev = None

'''

class Solution:
    def insertAtPos(self, head, p, x):
        ansNode = Node(x)
        temp = head
        
        for _ in range(0,p):
            temp = temp.next
            
        nextNode = temp.next
        temp.next = ansNode
        ansNode.prev = temp
        ansNode.next = nextNode
    
        return head
            
        
        
            
            
        
