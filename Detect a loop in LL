# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def hasCycle(self, head: Optional[ListNode]) -> bool:
        l = list()
        temp = head
        while temp != None:
            l.append(temp)
            if temp.next in l:
                return True
            temp = temp.next
        return False
